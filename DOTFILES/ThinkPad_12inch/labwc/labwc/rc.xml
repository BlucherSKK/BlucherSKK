<?xml version="1.0"?>
<!--
  This file contains all blucher's settings for labwc and it was updating 26 Dec 2025.
-->
<labwc_config>
  <!--░█▀▀░█▀█░█▀▄░█▀▀
      ░█░░░█░█░█▀▄░█▀▀
      ░▀▀▀░▀▀▀░▀░▀░▀▀▀-->
  <core>
    <decoration>server</decoration>
    <gap>0</gap>
    <adaptiveSync>no</adaptiveSync>
    <allowTearing>no</allowTearing>
    <autoEnableOutputs>yes</autoEnableOutputs>
    <reuseOutputMode>no</reuseOutputMode>
    <xwaylandPersistence>yes</xwaylandPersistence>
    <primarySelection>yes</primarySelection>
  </core>
  
  <placement policy="center"/>
  
  <windowSwitcher>
    <osd output="cursor" />
  </windowSwitcher>
  
  <resistance>
    <screenEdgeStrength>20</screenEdgeStrength>
    <windowEdgeStrength>20</windowEdgeStrength>
    <unSnapThreshold>20</unSnapThreshold>
    <unMaximizeThreshold>150</unMaximizeThreshold>
  </resistance>
  
  <resize>
    <popupShow>Never</popupShow>
    <drawContents>yes</drawContents>
    <minimumArea>8</minimumArea>
  </resize>
  
  <focus>
    <followMouse>yes</followMouse>
    <followMouseRequiresMovement>yes</followMouseRequiresMovement>
    <raiseOnFocus>no</raiseOnFocus>
  </focus>
  
  <snapping>
    <range>10</range>
    <cornerRange>50</cornerRange>
    <overlay enabled="yes">
      <delay inner="500" outer="500"/>
    </overlay>
    <topMaximize>yes</topMaximize>
    <notifyClient>always</notifyClient>
  </snapping>
  
  <desktops>
    <popupTime>1000</popupTime>
    <names>
      <name>1</name>
      <name>2</name>
      <name>3</name>
    </names>
  </desktops>
  
  <margin top="" bottom="10" left="10" right="10" output="eDP-1"/>

  
  <!--░▀█▀░█░█░█▀▀░█▄█░█▀▀
      ░░█░░█▀█░█▀▀░█░█░█▀▀
      ░░▀░░▀░▀░▀▀▀░▀░▀░▀▀▀ -->
  <theme>
    <name>Cyan</name>
    <icon/>
    <fallbackAppIcon>labwc</fallbackAppIcon>
    <titlebar>
      <layout/>
      <showTitle>no</showTitle>
    </titlebar>
    <cornerRadius>8</cornerRadius>
    <keepBorder>no</keepBorder>
    <dropShadows>no</dropShadows>
    <dropShadowsOnTiled>no</dropShadowsOnTiled>
    <font name="Iosevka" size="14"/>
  </theme>






  
  <!--
██╗  ██╗███████╗██╗   ██╗██████╗  ██████╗  █████╗ ██████╗ ██████╗ 
██║ ██╔╝██╔════╝╚██╗ ██╔╝██╔══██╗██╔═══██╗██╔══██╗██╔══██╗██╔══██╗
█████╔╝ █████╗   ╚████╔╝ ██████╔╝██║   ██║███████║██████╔╝██║  ██║
██╔═██╗ ██╔══╝    ╚██╔╝  ██╔══██╗██║   ██║██╔══██║██╔══██╗██║  ██║
██║  ██╗███████╗   ██║   ██████╔╝╚██████╔╝██║  ██║██║  ██║██████╔╝
╚═╝  ╚═╝╚══════╝   ╚═╝   ╚═════╝  ╚═════╝ ╚═╝  ╚═╝╚═╝  ╚═╝╚═════╝ 
  -->
  <keyboard>
    <layoutScope>global</layoutScope>
    <repeatRate>25</repeatRate>
    <repeatDelay>600</repeatDelay>
    <keybind key="W-Tab">
      <action name="NextWindow"/>
    </keybind>
    <keybind key="W-d">
      <action name="ForEach">
        <query>
            </query>
        <then>
          <action name="Iconify"/>
        </then>
      </action>
    </keybind>
    <keybind key="A-Tab">
      <action name="NextWindow"/>
    </keybind>
    <keybind key="W-S-Down">
      <action name="Iconify"/>
    </keybind>
    <keybind key="W-Return">
      <action name="Execute" command="lab-sensible-terminal"/>
    </keybind>
    <keybind key="W-S-r">
      <action name="Execute" command="labwc --reconfigure"/>
    </keybind>
    <keybind key="Print">
      <action name="Execute" command="~/.scripts/scren.sh"/>
    </keybind>
    <keybind key="W-S-w">
      <action name="Execute" command="pkill waybar"/>
      <action name="Execute" command="waybar"/>
    </keybind>
    <keybind key="C-A-t">
      <action name="Execute" command="kitty"/>
    </keybind>
    <keybind key="W-b">
      <action name="Execute">
        <command>kitty --start-as=maximized -o background_tint=1 -e btop</command>
      </action>
    </keybind>
    <keybind key="ISO_Next_Group">
      <action name="Execute" command="~/.scripts/switch_layout.sh"/>
    </keybind>
    <keybind key="W-v">
      <action name="Execute" command="vesktop"/>
    </keybind>
    <keybind key="W-e">
      <action name="Execute" command="emacs -mm"/>
    </keybind>
    <keybind key="Super_L" onRelease="yes">
      <action name="Execute" command="~/.scripts/rofi_exec.sh launcher"/>
    </keybind>
    <keybind key="W-x">
      <action name="Execute" command="~/.scripts/rofi_exec.sh powermenu"/>
    </keybind>
    <keybind key="W-w">
      <action name="Execute" command="firefox"/>
    </keybind>
    <keybind key="W-f">
      <action name="Execute" command="nemo --geometry=2560x1440"/>
    </keybind>
    <keybind key="W-c">
      <action name="Close"/>
    </keybind>
    <keybind key="W-Up">
      <action name="ToggleMaximize"/>
    </keybind>
    <keybind key="W-S-Up">
      <action name="ToggleFullscreen"/>
    </keybind>
    <keybind key="W-Left">
      <action name="SnapToEdge" direction="left"/>
    </keybind>
    <keybind key="W-Right">
      <action name="SnapToEdge" direction="right"/>
    </keybind>
    <keybind key="F10">
      <action name="Execute" command="~/.scripts/random_wallpaper.sh nsfw ++"/>
    </keybind>
    <keybind key="F12">
      <action name="Execute" command="~/.scripts/random_wallpaper.sh sfw ++"/>
    </keybind>
    <keybind key="F9">
      <action name="Execute" command="~/.scripts/random_wallpaper.sh nsfw --"/>
    </keybind>
    <keybind key="F11">
      <action name="Execute" command="~/.scripts/random_wallpaper.sh sfw --"/>
    </keybind>
    <keybind key="W-Down">
      <action name="ResizeTo" width="1280" height="720"/>
      <action name="AutoPlace" policy="center"/>
    </keybind>
    <keybind key="A-Space">
      <action name="ShowMenu" menu="client-menu" atCursor="no"/>
    </keybind>
    <keybind key="XF86_AudioLowerVolume">
      <action name="Execute" command="amixer sset Master 5%-"/>
    </keybind>
    <keybind key="XF86_AudioRaiseVolume">
      <action name="Execute" command="amixer sset Master 5%+"/>
    </keybind>
    <keybind key="XF86_AudioMute">
      <action name="Execute" command="amixer sset Master toggle"/>
    </keybind>
    <keybind key="XF86_MonBrightnessUp">
      <action name="Execute" command="brightnessctl set +10%"/>
    </keybind>
    <keybind key="XF86_MonBrightnessDown">
      <action name="Execute" command="brightnessctl set 10%-"/>
    </keybind>
  </keyboard>




  
  <!--
███╗   ███╗ ██████╗ ██╗   ██╗███████╗███████╗
████╗ ████║██╔═══██╗██║   ██║██╔════╝██╔════╝
██╔████╔██║██║   ██║██║   ██║███████╗█████╗  
██║╚██╔╝██║██║   ██║██║   ██║╚════██║██╔══╝  
██║ ╚═╝ ██║╚██████╔╝╚██████╔╝███████║███████╗
╚═╝     ╚═╝ ╚═════╝  ╚═════╝ ╚══════╝╚══════╝
  -->
  <mouse>
    <dragThreshold>4</dragThreshold>
    <doubleClickTime>200</doubleClickTime>
    <screenEdgeWarpTime>400</screenEdgeWarpTime>
    <screenEdgeWarpMouse>false</screenEdgeWarpMouse>
    <context name="Frame">
      <mousebind action="Press" button="A-Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="W-Left">
        <action name="Move"/>
      </mousebind>
      <mousebind action="Press" button="A-Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Drag" button="A-Right">
        <action name="Resize"/>
      </mousebind>
    </context>
    <context name="Titlebar">
      <mousebind action="Press" button="A-Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="A-Left">
        <action name="Move"/>
      </mousebind>
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="DoubleClick" button="Left">
        <action name="ToggleMaximize"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
      <mousebind action="Press" button="Middle">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="ShowMenu">
          <menu>client-layer-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="Client">
      <mousebind action="Press" button="A-Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Drag" button="A-Left">
        <action name="Move"/>
      </mousebind>
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
      </mousebind>
    </context>
    <context name="Icon">
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
        <action name="ShowMenu">
          <menu>client-menu</menu>
        </action>
      </mousebind>
    </context>
    <context name="AllDesktops">
      <mousebind action="Click" button="Left">
        <action name="ToggleOmnipresent"/>
      </mousebind>
    </context>
    <context name="Shade">
      <mousebind action="Click" button="Left">
        <action name="ToggleShade"/>
      </mousebind>
    </context>
    <context name="Iconify">
      <mousebind action="Click" button="Left">
        <action name="Iconify"/>
      </mousebind>
    </context>
    <context name="Maximize">
      <mousebind action="Press" button="Left">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="Focus"/>
        <action name="Raise"/>
        <action name="Unshade"/>
      </mousebind>
      <mousebind action="Click" button="Left">
        <action name="ToggleMaximize"/>
      </mousebind>
      <mousebind action="Click" button="Right">
        <action name="ToggleMaximize">
          <direction>horizontal</direction>
        </action>
      </mousebind>
    </context>
    <context name="Close">
      <mousebind action="Click" button="Left">
        <action name="Close"/>
      </mousebind>
    </context>
    <context name="Desktop">
      <mousebind action="Click" button="Left">
        <action name="Focus"/>
      </mousebind>
    </context>
    <context name="Root">
      <mousebind action="Press" button="Middle">
        <action name="ShowMenu">
          <menu>client-list-combined-menu</menu>
        </action>
      </mousebind>
      <mousebind action="Press" button="A-Right">
        <action name="ShowMenu">
          <menu>client-list-menu</menu>
        </action>
      </mousebind>
      <mousebind action="Press" button="Right">
        <action name="ShowMenu">
          <menu>root-menu</menu>
        </action>
      </mousebind>
    </context>
  </mouse>


  

  <!--
██╗    ██╗██╗███╗   ██╗██████╗  ██████╗ ██╗    ██╗    ██████╗ ██╗   ██╗██╗     ███████╗███████╗
██║    ██║██║████╗  ██║██╔══██╗██╔═══██╗██║    ██║    ██╔══██╗██║   ██║██║     ██╔════╝██╔════╝
██║ █╗ ██║██║██╔██╗ ██║██║  ██║██║   ██║██║ █╗ ██║    ██████╔╝██║   ██║██║     █████╗  ███████╗
██║███╗██║██║██║╚██╗██║██║  ██║██║   ██║██║███╗██║    ██╔══██╗██║   ██║██║     ██╔══╝  ╚════██║
╚███╔███╔╝██║██║ ╚████║██████╔╝╚██████╔╝╚███╔███╔╝    ██║  ██║╚██████╔╝███████╗███████╗███████║
 ╚══╝╚══╝ ╚═╝╚═╝  ╚═══╝╚═════╝  ╚═════╝  ╚══╝╚══╝     ╚═╝  ╚═╝ ╚═════╝ ╚══════╝╚══════╝╚══════╝
-->
  <windowRules>
    <windowRule identifier="*" serverDecoration="no" />\
    <windowRule identifier="firefox" serverDecoration="no">
      <action name="ToggleMaximaze"/>
    </windowRule>
    <windowRule identifier="nemo" serverDecoration="no">
      <action name="ToggleMaximaze"/>
    </windowRule>
  </windowRules>



  
  <!--
███╗   ███╗███████╗███╗   ██╗██╗   ██╗
████╗ ████║██╔════╝████╗  ██║██║   ██║
██╔████╔██║█████╗  ██╔██╗ ██║██║   ██║
██║╚██╔╝██║██╔══╝  ██║╚██╗██║██║   ██║
██║ ╚═╝ ██║███████╗██║ ╚████║╚██████╔╝
╚═╝     ╚═╝╚══════╝╚═╝  ╚═══╝ ╚═════╝ 
-->
  <menu>
    <ignoreButtonReleasePeriod>250</ignoreButtonReleasePeriod>
    <showIcons>yes</showIcons>
  </menu>
</labwc_config>
